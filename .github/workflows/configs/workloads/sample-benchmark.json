{
    "schema_version": "1.0",
    "benchmark_profile": {
        "name": "Standard Performance Test",
        "description": "Compare client library performance with GET/SET workload",
        "version": "1.0.0"
    },
    "phases": [
        {
            "id": "WARMUP",
            "description": "Warmup phase",
            "connections": 50,
            "cps_limit": -1,
            "rps_limit": -1,
            "completion": {
                "type": "duration",
                "seconds": 10
            },
            "keyspace": {
                "keys_count": 100000,
                "key_size_bytes": 16,
                "key_prefix": "bench:",
                "generation_alg": "sequential_int"
            },
            "commands": [
                {
                    "command": "set",
                    "weight": 1,
                    "data_size_bytes": 256
                }
            ]
        },
        {
            "id": "STEADY",
            "description": "Load phase",
            "connections": 500,
            "cps_limit": 100,
            "rps_limit": 10000,
            "completion": {
                "type": "requests",
                "requests": 1000000
            },
            "keyspace": {
                "seed": 0,
                "keys_count": 100000,
                "key_size_bytes": 16,
                "key_prefix": "bench:",
                "generation_alg": "uniform_rand"
            },
            "commands": [
                {
                    "command": "set",
                    "weight": 0.3,
                    "data_size_bytes": 256
                },
                {
                    "command": "get",
                    "weight": 0.7
                }
            ]
        }
    ]
}